{
  "name": "custom-portfolio",
  "version": "1.0.0",
  "description": "This application creates a way for doctors to communicate with patients and vise versa.",
  "main": "index.js",
  "scripts": {
    "test": "run-s build jest",
    "jest": "jest",
    "watch": "webpack --config webpack.dev.js --watch",
    "build": "webpack --config webpack.prod.js",
    "start:dev": "npm-run-all --parallel dev-server watch",
    "start": "run-s build prod-server",
    "dev-server": "nodemon server/server.js",
    "prod-server": "node server/server.js 80",
    "coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "lint": "eslint ."
  },
  "author": "Timothy Searcy",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "babel-core": "^6.26.0",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "coveralls": "^3.0.2",
    "css-loader": "^0.28.9",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^22.0.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^3.1.0",
    "fetch-mock": "^7.2.5",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.1.0",
    "mini-css-extract-plugin": "^0.4.1",
    "mockdate": "^2.0.2",
    "node-sass": "^4.7.2",
    "nodemon": "^1.18.3",
    "npm-run-all": "^4.1.3",
    "postcss-loader": "^2.0.10",
    "precss": "^3.1.0",
    "react-test-renderer": "^16.4.0",
    "redux-mock-store": "^1.5.3",
    "regenerator-runtime": "^0.11.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
    "superagent": "^4.0.0",
    "supertest": "^3.3.0",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "url-loader": "^1.0.0",
    "webpack": "^4.16.4",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "bootstrap": "^4.0.0",
    "chart.js": "^1.1.1",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "jquery": "^3.3.1",
    "moment": "^2.22.1",
    "multer": "^1.4.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "popper.js": "^1.12.9",
    "react": "^16.2.0",
    "react-chartjs": "^1.2.0",
    "react-datepicker": "^1.4.1",
    "react-dom": "^16.2.0",
    "react-html-email": "^3.0.0",
    "react-modal": "^3.4.5",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-select": "^1.2.1",
    "react-spinners": "^0.3.2",
    "react-textarea-autosize": "^7.1.0-1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "webpack-dev-server": "^3.1.10"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "moduleDirectories": [
      "node_modules",
      "src/render-component"
    ],
    "testURL": "http://localhost/",
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/dist/**",
      "!**/webpack*",
      "!**/coverage/**",
      "!server/server.js"
    ],
    "setupFiles": [
      "./setup-tests.js"
    ]
  }
}
